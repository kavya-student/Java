public class Question{
    public static void main(String[] args){
        int totaldays = 31;
        int count =0;
        for(int day = 1;day<=totaldays;day++){
            if(day % 2==0){
                count++;
            }
        }
        System.out.println("Kavya can go out on " + count + " days in August.");
    }
}
  
